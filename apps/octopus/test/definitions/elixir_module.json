{
  "name": "my_module",
  "execution": {
    "type": "compile",
    "start": {
      "code": [
        "defmodule TheModule do",
        "def hello(name) do",
        "\"Hello #{name}\"",
        "end",
        "def add(x, y), do: x + y",
        "end"
      ]
    }
  },
  "interface": {
    "type": "code",
    "add": {
      "call": {
        "function": "add",
        "module": "Octopus.Sandbox.TheModule"
      },
      "input": {
        "args": {
          "x": null,
          "y": null
        },
        "transform": {
          "eval": true,
          "template": "[<%= args[\"x\"] %>, <%= args[\"y\"] %>]"
        }
      },
      "output": "as_is"
    },
    "hello": {
      "call": {
        "function": "hello",
        "module": "Octopus.Sandbox.TheModule"
      },
      "input": {
        "args": {
          "name": null
        },
        "transform": {
          "eval": false,
          "template": "<%= args[\"name\"] %>"
        }
      },
      "output": "as_is"
    }
  }
}
